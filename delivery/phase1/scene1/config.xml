<world>
    <window width="1024" height="1024"/>
    <camera>
        <position x="4" y="3" z="2"/>
        <lookAt x="0" y="0" z="0"/>
        <up x="0" y="1" z="0"/>
        <projection fov="60" near="1" far="1000" />
    </camera>
    <group>
        <transform>
            <scale x="0.1" y="0.1" z="0.1" />
            <translate x="-15" y="0" z="-15" />
        </transform>
        <models>
            <model file="plane.obj" />
        </models>
        <group>
            <transform>
                <translate x="10" y="0" z="10" />
            </transform>
            <models>
                <model file="cone.obj" />
            </models>
            <group>
                <transform>
                    <translate x="10" y="5" z="10" />
                </transform>
                <models>
                    <model file="sphere.obj" />
                </models>
                <group>
                    <transform>
                        <translate x="10" y="0" z="10" />
                    </transform>
                    <models>
                        <model file="box.obj" />
                    </models>
                </group>
            </group>
        </group>
    </group>
</world>